<script type="text/javascript">

        $(function() {

            $('.rightBtn .btnGuide').text('사용신청안내');

            /* 전통디자인 > 문양코드복사 */
            var copyButton = $('.copyPatternCode');
            if (copyButton.size() > 0){
                copyPatternCode(copyButton);
            }
        });

        var copyPatternCode = function(_target) {
            _target.parent().zclip({
                path : "../js/ZeroClipboard.swf",
                copy : function(){return $(this).children().attr('data-code');}
            });
        }

        function org_down(did){
            var applyInfo = null;

            if(applyInfo == null){
                location.href='/tradition/autoGuideB02.do?type=org&pattern_type=PG03&chkOrgImg='+did;
            }else{

                var param = "type=org&pattern_type=PG03&chkOrgImg="+did;

                jQuery.ajax({
                    type : "POST",
                    url: '/tradition/autoPatternDownFileNew.do',
                    data: param,
                    contentType: "application/x-www-form-urlencoded; charset=UTF-8",
                    cache: false,
                    dataType: "json",
                    success : function(success) {
                        file_check2(success.message,did);
                    }
                    ,error : function(result) {
                        alert("fail");
                    }
                });

                // $('#type').val('org');
                // $('#pattern_type').val('PG03');
                // $('#chkOrgImg').val(did);
                //
                // $('#patternDownForm').submit();
            }
        }
        function file_check2(downloadkey,did){
            window.open('/ptn/download.do?downloadkey=' + downloadkey, '_blank');

            var param = "type=org&pattern_type=PG03&chkOrgImg="+did;
            var	furl = '/tradition/autoPatternDownFileCheck.do';

            $.ajax({
                type: "POST",
                url: furl,
                data: param,
                dataType : "json",
                success: function(data) {
                    //20.12.31 없는 함수를 호출..오류나서 주석처리함
                    //returnMessage(data.total_count,data.exist_count,data.file_name);

                    // 20.12.22 와이즈트래커 추가
                    startDownloadWiseTracking(did,'G12');
                }
            });
        }


        function downLoadDesign(did) {
            /**
             <sec:authorize ifNotGranted="ROLE_USER">
             alert("로그인하시면 \n문양을 다운로드 받으실 수 있습니다.");
             location.href="/member/login.do?redirect_url="+escape(this.location);
             </sec:authorize>
             <sec:authorize ifAnyGranted="ROLE_USER">
             location.href='designPatternDownFile.do?seq='+did;
             </sec:authorize>
             **/
            window.open('/ptn/image.do?type=PG03&seq='+did);

            // 20.12.22 와이즈트래커 추가
            startDownloadWiseTracking(did,'G11');
        }
    </script>
